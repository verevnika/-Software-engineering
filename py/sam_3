a = int(input())
if 0 <= a <= 10:
    if a <= 3:
        print("Число в диапозоне от 0 до 3 включительно")
    elif 3 < a < 6:
        print("Число в диапозоне от 3 до 6")
    else:
        print("Число в диапозоне от 6 до 10 включительно  ")
else:
    print("Число не в ходит в диапозон от 0 до 10 включительно")
